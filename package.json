{
    "name": "format-html-in-php",
    "displayName": "Format HTML in PHP",
    "description": "Format nested HTML in PHP files for vscode",
    "version": "1.3.2",
    "publisher": "rifi2k",
    "repository": {
        "url": "https://github.com/RiFi2k/format-html-in-php"
    },
    "license": "Unlicense",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other",
        "Programming Languages",
        "Formatters"
    ],
    "keywords": [],
    "activationEvents": [
        "onLanguage:php"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "js-beautify": "^1.8.0-rc7",
        "lodash.get": "^4.4.2"
    }
}
